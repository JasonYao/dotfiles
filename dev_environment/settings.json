{
    "version": "alpha",
    "schema": "developer_environments",
    "environments": [
        {
            "language": "python",
            "versions": ["3.7.2", "3.6.8", "2.7.15"],
            "environment_manager": {
                "name": "pyenv",
                "plugins": [
                    {
                        "name": "pyenv-virtualenv",
                        "virtual_environments": [
                            {
                                "version": "3.7.2",
                                "name": "default-3.7.2"
                            },
                            {
                                "version": "3.6.8",
                                "name": "default-3.6.8"
                            },
                            {
                                "version": "2.7.15",
                                "name": "default-2.7.15"
                            }
                        ]
                    }
                ]
            }
        },
        {
            "language": "java",
            "versions": ["8", "latest"],
            "environment_manager": {
                "name": "jenv"
            }
        }
    ]
}
